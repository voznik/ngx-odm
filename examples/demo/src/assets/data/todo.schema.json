{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Todo",
  "description": "Todo Schema",
  "required": ["id", "title", "createdAt"],
  "version": 0,
  "properties": {
    "id": {
      "type": "string",
      "title": "Id",
      "primary": true,
      "pattern": "^(.*)$"
    },
    "title": {
      "type": "string",
      "title": "Title"
    },
    "completed": {
      "type": "boolean",
      "title": "Done"
    },
    "createdAt": {
      "type": "number",
      "title": "Date Created"
    }
  },
  "indexes": ["createdAt"]
}
