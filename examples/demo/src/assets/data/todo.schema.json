{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Todo",
  "description": "Todo Schema",
  "required": ["id", "title", "createdAt"],
  "version": 1,
  "properties": {
    "id": {
      "type": "string",
      "title": "Id",
      "pattern": "^(.*)$",
      "maxLength": 36
    },
    "title": {
      "type": "string",
      "title": "Title"
    },
    "completed": {
      "type": "boolean",
      "title": "Done"
    },
    "createdAt": {
      "type": "number",
      "title": "Date Created",
      "format": "date-time"
    },
    "last_modified": {
      "type": "number",
      "title": "Last Modified",
      "multipleOf": 1
    }
  },
  "__indexes": ["createdAt"],
  "primaryKey": "id"
}
