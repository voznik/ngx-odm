{
  "$schema": "https://json.schemastore.org/package",
  "name": "ngx-odm",
  "version": "0.0.0-dev",
  "private": false,
  "description": "Angular ODMs for NoSql DBs like RxDB, Kinto",
  "keywords": [
    "angular",
    "rxjs",
    "rxdb",
    "nosql",
    "indexeddb",
    "pouchdb",
    "typescript",
    "kinto",
    "replication"
  ],
  "homepage": "https://github.com/voznik/ngx-odm#readme",
  "bugs": {
    "url": "https://github.com/voznik/ngx-odm/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voznik/ngx-odm.git"
  },
  "license": "MIT",
  "author": {
    "name": "voznik",
    "url": "https://github.com/voznik"
  },
  "contributors": [
    {
      "name": "voznik",
      "url": "https://github.com/voznik"
    }
  ],
  "main": "decorate-angular-cli.js",
  "scripts": {
    "nx": "nx",
    "ts-node": "ts-node",
    "changelog": "conventional-changelog",
    "nyc": "nyc",
    "prettier": "prettier",
    "start": "nx run demo:serve",
    "build:demo": "nx run demo:build --base-href=/ngx-odm/",
    "build:demo:sourcemaps": "nx run demo:build --configuration=production --sourceMap=true --statsJson",
    "build": "nx run rxdb:build --configuration=production",
    "test": "nx run rxdb:test --ci --run-in-band --parallel --pass-with-no-tests --collectCoverage && npm run test:merge",
    "test:merge": "./node_modules/.bin/lcov-result-merger 'coverage/packages/*.lcov' coverage/lcov.info",
    "lint": "nx run rxdb:lint",
    "e2e": "ng e2e",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:check": "nx format:check --base=master --head=HEAD",
    "update": "ng update @nrwl/workspace",
    "dep-graph": "nx dep-graph",
    "analyze": "node --max_old_space_size=8000 ./node_modules/.bin/source-map-explorer dist/demo/{vendor,main}.*.js{,.map} -m --no-border-checks",
    "analyze:file": "node --max_old_space_size=8000 ./node_modules/.bin/source-map-explorer dist/demo/{vendor,main}.*.js{,.map} -m --no-border-checks --html dist/out/result.html",
    "changelog:write": "conventional-changelog -p angular -i CHANGELOG.md -s --config=tools/scripts/conventional-changelog-config.js --skip-unstable",
    "compodoc": "nx run tools:compodoc"
  },
  "dependencies": {
    "@angular-architects/ngrx-toolkit": "^0.0.3",
    "@angular/animations": "16.2.5",
    "@angular/common": "16.2.5",
    "@angular/compiler": "16.2.5",
    "@angular/core": "16.2.5",
    "@angular/forms": "16.2.5",
    "@angular/platform-browser": "16.2.5",
    "@angular/platform-browser-dynamic": "16.2.5",
    "@angular/router": "16.2.5",
    "@angular/service-worker": "16.2.5",
    "@ngrx/component": "^16.3.0",
    "@ngrx/signals": "^17.0.1",
    "@nrwl/angular": "16.8.1",
    "compare-versions": "^6.1.0",
    "normalize.css": "8.0.1",
    "reflect-metadata": "0.1.13",
    "rxdb": "^15.0.0-beta.47",
    "rxjs": "7.8.1",
    "todomvc-app-css": "2.4.3",
    "todomvc-common": "1.0.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.2.0",
    "@angular-devkit/core": "~16.2.0",
    "@angular-devkit/schematics": "~16.2.0",
    "@angular-eslint/eslint-plugin": "~16.0.0",
    "@angular-eslint/eslint-plugin-template": "~16.0.0",
    "@angular-eslint/template-parser": "~16.0.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "~16.2.0",
    "@angular/language-service": "~16.2.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@compodoc/compodoc": "1.1.21",
    "@jscutlery/semver": "^3.4.1",
    "@nrwl/eslint-plugin-nx": "16.8.1",
    "@nrwl/jest": "16.8.1",
    "@nrwl/tao": "16.8.1",
    "@nrwl/web": "16.8.1",
    "@nrwl/workspace": "16.8.1",
    "@nx/angular": "^16.8.1",
    "@nx/eslint-plugin": "16.8.1",
    "@nx/jest": "16.8.1",
    "@nx/js": "16.8.1",
    "@nx/linter": "16.8.1",
    "@nx/workspace": "16.8.1",
    "@schematics/angular": "~16.2.0",
    "@twittwer/compodoc": "1.10.0",
    "@types/dotenv": "8.2.0",
    "@types/fs-extra": "^11.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@types/webpack": "4.41.25",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "conventional-changelog-cli": "^4.1.0",
    "document-register-element": "1.14.10",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "8.0.1",
    "eslint": "~8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-rxjs": "=5.0.3",
    "eslint-plugin-rxjs-angular": "=2.0.1",
    "fs-extra": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-junit": "^16.0.0",
    "jest-localstorage-mock": "2.4.6",
    "jest-mock-fetch": "^2.0.5",
    "jest-preset-angular": "~13.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lcov-result-merger": "^3.1.0",
    "lint-staged": "^14.0.1",
    "ng-packagr": "16.2.3",
    "nx": "16.8.1",
    "nyc": "^15.1.0",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "3.0.3",
    "process": "^0.11.10",
    "rimraf": "5.0.1",
    "semver": "^7.3.4",
    "setimmediate": "^1.0.5",
    "source-map-explorer": "2.5.1",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "type-fest": "^4.3.1",
    "typescript": "~5.1.3"
  },
  "engines": {
    "node": ">=16"
  },
  "npmScope": "@ngx-odm",
  "packageScope": "@ngx-odm",
  "overrides": {
    "@nrwl/eslint-plugin-nx": {
      "eslint-config-prettier": "9.0.0"
    }
  }
}
